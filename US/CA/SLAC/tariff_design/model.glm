// Model glm for testing

#input "config.csv" -f config -t config

// Clock 
clock {
	timezone ${TIMEZONE};
	starttime ${STARTTIME};
	stoptime ${STOPTIME};
}

module tape;

// Climate module needed
#input "${WEATHER_STATION}.tmy3"

module powerflow;

object triplex_meter {
	name "test_meter";
	measured_energy_delta_timestep "3600";

	// Added b/c errors in Docker?
	phases AS;
	nominal_voltage 120;
}

module residential;

object house {
	parent "test_meter";
	name "test_house";
}